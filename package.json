{
  "name": "puzzle",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:with-logging-frequently-used-files": "vite --debug transform",
    "start": "vite",
    "build": "vite build",
    "build:with-type-safe": "tsc && vite build",
    "preview": "vite preview",
    "preview-coverage": "vite preview -c vite-coverage-preview.config.ts",
    "test": "npm run test-unit && npm run test-e2e",
    "test-unit:ui": "vitest --ui",
    "test-unit:ui-coverage": "vitest --ui --coverage.enabled=true",
    "test-unit": "vitest run",
    "test-unit:coverage": "npm run test-unit -- --coverage",
    "test-e2e": "npm run test-e2e:smoke && npm run test-e2e:normal",
    "test-e2e:setup": "npx playwright test --project=setup",
    "test-e2e:smoke": "npx playwright test --project=smoke",
    "test-e2e:normal": "npx playwright test --project=normal",
    "test-e2e:list": "npx playwright test --list",
    "test-e2e:show-report": "npx playwright show-report",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:advanced": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "type-check": "tsc --noEmit",
    "ci:precheck": "npm run prettier:check && npm run lint && npm run lint:css && npm run test",
    "dependencies-check": "npx npm-check-updates",
    "dependencies-update": "npx npm-check-updates -u",
    "source:compress": "node scripts/zip-source.mjs",
    "source:compress-all": "node scripts/zip-source-all.mjs",
    "source:decompress": "node scripts/unzip-source.mjs"
  },
  "dependencies": {
    "@fontsource-variable/open-sans": "^5.0.29",
    "@types/seedrandom": "^3.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-full-screen": "1.1.1",
    "react-svg": "16.1.34",
    "seedrandom": "^3.0.5",
    "svgo": "^3.3.2",
    "typescript": "^5.4.5",
    "web-vitals": "^4.1.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/archiver": "^6.0.2",
    "@types/jest": "^29.5.12",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "archiver": "^7.0.1",
    "colord": "^2.9.3",
    "decompress": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-testing-library": "^6.2.2",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-image-snapshot": "^6.4.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.2",
    "snapshot-diff": "^0.10.0",
    "stylelint": "^16.6.1",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard": "^36.0.0",
    "ts-jest": "^29.1.5",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4",
    "vitest-canvas-mock": "^0.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
